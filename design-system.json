{
  "designSystem": {
    "name": "Nike Mobile App Design System",
    "version": "1.0.0",
    "description": "Design system extracted from Nike mobile app interface",

    "colors": {
      "primary": {
        "black": "#000000",
        "white": "#FFFFFF"
      },
      "neutral": {
        "gray-50": "#F9F9F9",
        "gray-100": "#F5F5F5",
        "gray-200": "#E5E5E5",
        "gray-300": "#D4D4D4",
        "gray-400": "#A3A3A3",
        "gray-500": "#737373",
        "gray-600": "#525252",
        "gray-700": "#404040",
        "gray-800": "#262626",
        "gray-900": "#171717"
      },
      "accent": {
        "orange": "#FF6B35",
        "red": "#DC2626"
      },
      "background": {
        "primary": "#FFFFFF",
        "secondary": "#F9F9F9",
        "card": "#FFFFFF"
      },
      "text": {
        "primary": "#000000",
        "secondary": "#737373",
        "tertiary": "#A3A3A3"
      }
    },

    "typography": {
      "fontFamilies": {
        "primary": "Helvetica Neue, Arial, sans-serif",
        "display": "Futura, Helvetica Neue, Arial, sans-serif"
      },
      "fontSizes": {
        "xs": "12px",
        "sm": "14px",
        "base": "16px",
        "lg": "18px",
        "xl": "20px",
        "2xl": "24px",
        "3xl": "30px",
        "4xl": "36px"
      },
      "fontWeights": {
        "normal": 400,
        "medium": 500,
        "semibold": 600,
        "bold": 700,
        "extrabold": 800
      },
      "lineHeights": {
        "tight": 1.2,
        "normal": 1.4,
        "relaxed": 1.6
      },
      "textStyles": {
        "greeting": {
          "fontSize": "30px",
          "fontWeight": 700,
          "lineHeight": 1.2,
          "color": "#000000"
        },
        "sectionTitle": {
          "fontSize": "20px",
          "fontWeight": 600,
          "lineHeight": 1.3,
          "color": "#000000"
        },
        "sectionSubtitle": {
          "fontSize": "16px",
          "fontWeight": 400,
          "lineHeight": 1.4,
          "color": "#737373"
        },
        "productTitle": {
          "fontSize": "16px",
          "fontWeight": 600,
          "lineHeight": 1.3,
          "color": "#000000"
        },
        "productSubtitle": {
          "fontSize": "14px",
          "fontWeight": 400,
          "lineHeight": 1.4,
          "color": "#737373"
        },
        "price": {
          "fontSize": "16px",
          "fontWeight": 600,
          "lineHeight": 1.3,
          "color": "#000000"
        },
        "promoTitle": {
          "fontSize": "18px",
          "fontWeight": 700,
          "lineHeight": 1.3,
          "color": "#000000"
        },
        "promoDescription": {
          "fontSize": "14px",
          "fontWeight": 400,
          "lineHeight": 1.5,
          "color": "#737373"
        },
        "tabLabel": {
          "fontSize": "10px",
          "fontWeight": 500,
          "lineHeight": 1.2,
          "color": "#737373"
        },
        "tabLabelActive": {
          "fontSize": "10px",
          "fontWeight": 600,
          "lineHeight": 1.2,
          "color": "#000000"
        }
      }
    },

    "spacing": {
      "scale": "4px",
      "values": {
        "0": "0px",
        "1": "4px",
        "2": "8px",
        "3": "12px",
        "4": "16px",
        "5": "20px",
        "6": "24px",
        "8": "32px",
        "10": "40px",
        "12": "48px",
        "16": "64px",
        "20": "80px"
      },
      "semantic": {
        "pageMargin": "20px",
        "sectionSpacing": "32px",
        "cardPadding": "16px",
        "itemSpacing": "12px",
        "textSpacing": "8px"
      }
    },

    "layout": {
      "containers": {
        "page": {
          "paddingHorizontal": "20px",
          "paddingTop": "16px"
        },
        "section": {
          "marginBottom": "32px"
        },
        "card": {
          "padding": "0px",
          "marginBottom": "16px"
        }
      },
      "grid": {
        "productGrid": {
          "columns": 2,
          "gap": "12px",
          "itemAspectRatio": "1:1.4"
        }
      }
    },

    "borderRadius": {
      "none": "0px",
      "sm": "4px",
      "md": "8px",
      "lg": "12px",
      "xl": "16px",
      "2xl": "20px",
      "full": "9999px"
    },

    "shadows": {
      "none": "none",
      "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
      "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
      "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
      "card": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)"
    },

    "components": {
      "brandToggle": {
        "container": {
          "backgroundColor": "#F5F5F5",
          "borderRadius": "20px",
          "padding": "4px",
          "display": "flex",
          "gap": "4px"
        },
        "item": {
          "padding": "8px 16px",
          "borderRadius": "16px",
          "backgroundColor": "transparent"
        },
        "itemActive": {
          "backgroundColor": "#FFFFFF",
          "boxShadow": "0 1px 3px 0 rgba(0, 0, 0, 0.1)"
        }
      },

      "productCard": {
        "container": {
          "backgroundColor": "#FFFFFF",
          "borderRadius": "0px",
          "overflow": "hidden",
          "aspectRatio": "1:1.4"
        },
        "image": {
          "width": "100%",
          "aspectRatio": "1:1",
          "backgroundColor": "#F5F5F5",
          "objectFit": "cover"
        },
        "content": {
          "padding": "12px 0px"
        },
        "title": {
          "marginBottom": "4px"
        },
        "subtitle": {
          "marginBottom": "8px"
        }
      },

      "promoCard": {
        "container": {
          "backgroundColor": "#F5F5F5",
          "borderRadius": "12px",
          "padding": "16px",
          "display": "flex",
          "alignItems": "center",
          "gap": "12px"
        },
        "image": {
          "width": "80px",
          "height": "80px",
          "borderRadius": "8px",
          "backgroundColor": "#FFFFFF"
        },
        "content": {
          "flex": 1
        },
        "title": {
          "marginBottom": "4px"
        },
        "description": {
          "lineHeight": 1.5
        }
      },

      "tabBar": {
        "container": {
          "backgroundColor": "#FFFFFF",
          "borderTop": "1px solid #E5E5E5",
          "paddingTop": "8px",
          "paddingBottom": "20px",
          "paddingHorizontal": "20px"
        },
        "tabItem": {
          "display": "flex",
          "flexDirection": "column",
          "alignItems": "center",
          "gap": "4px",
          "flex": 1
        },
        "tabIcon": {
          "width": "24px",
          "height": "24px"
        },
        "tabLabel": {
          "textAlign": "center"
        },
        "tabIndicator": {
          "width": "4px",
          "height": "4px",
          "borderRadius": "2px",
          "backgroundColor": "#FF6B35",
          "position": "absolute",
          "top": "-2px"
        }
      },

      "sectionHeader": {
        "container": {
          "display": "flex",
          "justifyContent": "space-between",
          "alignItems": "flex-end",
          "marginBottom": "16px"
        },
        "titleGroup": {
          "display": "flex",
          "flexDirection": "column",
          "gap": "4px"
        },
        "viewAllLink": {
          "color": "#737373",
          "fontSize": "16px",
          "fontWeight": 400
        }
      },

      "searchIcon": {
        "width": "24px",
        "height": "24px",
        "color": "#000000"
      },

      "pageIndicator": {
        "container": {
          "display": "flex",
          "justifyContent": "center",
          "gap": "8px",
          "marginTop": "16px"
        },
        "dot": {
          "width": "8px",
          "height": "3px",
          "borderRadius": "2px",
          "backgroundColor": "#D4D4D4"
        },
        "dotActive": {
          "backgroundColor": "#000000"
        }
      }
    },

    "patterns": {
      "productListing": {
        "layout": "grid",
        "columns": 2,
        "gap": "12px",
        "cardStyle": "minimal",
        "imageAspectRatio": "1:1",
        "showPricing": true,
        "showSubtitle": true
      },

      "horizontalScroll": {
        "itemWidth": "auto",
        "gap": "12px",
        "paddingHorizontal": "20px",
        "showScrollIndicator": false
      },

      "sectionSpacing": {
        "betweenSections": "32px",
        "headerToContent": "16px",
        "withinContent": "12px"
      }
    },

    "animations": {
      "transitions": {
        "fast": "150ms ease-out",
        "normal": "200ms ease-out",
        "slow": "300ms ease-out"
      },
      "hover": {
        "scale": 1.02,
        "opacity": 0.9
      },
      "press": {
        "scale": 0.98,
        "opacity": 0.8
      }
    }
  }
}
